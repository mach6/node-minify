{
  "name": "node-minify",
  "version": "2.0.5",
  "description": "Javascript / CSS minifier based on Babili / Butternut / YUI Compressor / Google Closure Compiler / UglifyJS2 / Sqwish / Clean-css / CSSO",
  "homepage": "https://github.com/srod/node-minify",
  "author": {
    "name": "Rodolphe Stoclin",
    "email": "rodolphe@2clics.net",
    "url": "http://2clics.net"
  },
  "license": "MIT",
  "main": "lib/node-minify.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/srod/node-minify.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "compressor",
    "minify",
    "minifier",
    "yui",
    "gcc",
    "google",
    "closure",
    "compiler",
    "uglifyjs",
    "uglifyjs2",
    "windows",
    "sqwish",
    "clean-css",
    "csso",
    "babili",
    "butternut"
  ],
  "scripts": {
    "eslint": "eslint lib test || true",
    "pretest": "npm run eslint",
    "test-cov": "npm run pretest && jest --coverage",
    "test-cli": "node bin/cli.js --compressor babili --input 'examples/public/js/sample.js' --output 'examples/public/js-dist/babili-es6.js'",
    "test": "npm run test-cli && jest",
    "posttest": "npm run clean",
    "clean": "rm -f ./examples/public/dist/*",
    "clean-cov": "npm run clean && rm -Rf ./coverage",
    "release-patch": "npm version patch -m 'Bump %s' && git push --tags origin HEAD:master",
    "release-minor": "npm version minor -m 'Bump %s' && git push --tags origin HEAD:master",
    "release-major": "npm version major -m 'Bump %s' && git push --tags origin HEAD:master",
    "publish-latest": "npm publish",
    "publish-beta": "npm publish --tag beta"
  },
  "bin": {
    "node-minify": "bin/cli.js"
  },
  "dependencies": {
    "babel-core": "6.24.1",
    "babel-preset-babili": "0.0.12",
    "chalk": "1.1.3",
    "butternut": "0.4.6",
    "clean-css": "4.1.3",
    "csso": "3.1.1",
    "depd": "1.1.0",
    "glob": "7.1.2",
    "google-closure-compiler-js": "20170423.0.0",
    "gzip-size": "3.0.0",
    "meow": "3.7.0",
    "mkdirp": "0.5.1",
    "node-version": "1.0.0",
    "ora": "0.3.0",
    "pretty-bytes": "4.0.2",
    "sqwish": "0.2.2",
    "text-table": "0.2.0",
    "uglify-js": "3.0.10",
    "xtend": "4.0.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.24.1",
    "eslint": "3.19.0",
    "jest": "20.0.3"
  },
  "jest": {
    "notify": true,
    "verbose": true,
    "testEnvironment": "node"
  }
}
